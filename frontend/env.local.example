# RevoMotors Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Authentication (if using JWT tokens stored in localStorage)
# NEXT_PUBLIC_TOKEN_STORAGE_KEY=revomotors_token

# Feature Flags
NEXT_PUBLIC_ENABLE_HOT_LEADS=true
NEXT_PUBLIC_ENABLE_MARKETPLACE=true
NEXT_PUBLIC_ENABLE_AI_PRICING=true

# Analytics (optional)
# NEXT_PUBLIC_GA_ID=

# Sentry Error Tracking (optional)
# NEXT_PUBLIC_SENTRY_DSN=
